    <title>Gaurav's Website</title>


</head>
<body>
    @await Component.InvokeAsync("ClientNotifications.Notify")
    <!-- Preloader Start Here -->
    <div id="preloader"></div>
    <!-- Preloader End Here -->
    <div id="wrapper" class="wrapper bg-ash">
        <!-- Header Menu Area Start Here -->
        @await Component.InvokeAsync("HeaderMenu")
        <!-- Header Menu Area End Here -->
        <!-- Page Area Start Here -->
        <div class="dashboard-page-one" style="padding-top: 5%; height:1000px ">
            <!-- Sidebar Area Start Here -->
            @await Component.InvokeAsync("MenuBar")
            <!-- Sidebar Area End Here -->
            <div class="dashboard-content-one" id="main-div">


                @RenderBody()
            </div>
            @if (IsSectionDefined("Scripts"))
            {
                @RenderSection("Scripts", required: false);
            }
        </div>
        <!-- Page Area End Here -->
    </div>


    @*
        <div id="divLoading" class="divhide">

